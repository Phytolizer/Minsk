type :i3 = { w 3 } 

export
function :i3 $blit() {
@start
	%l0 =l alloc4 12
	%l1 =l alloc4 12

	storew 287454020, %l0
	%l04 =l add %l0, 4
	storew 1432778632, %l04
	%l08 =l add %l0, 8
	storew 2578103244, %l08

	# we expect that %l0 and %l1
	# are coalesced and the blit
	# goes backwards
	%l11 =l add %l1, 1
	blit %l0, %l11, 11

	storeb 221, %l1

	ret %l1
}

